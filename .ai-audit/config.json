{
	"standard": "VIBES",
	"standard_version": "1.0",
	"assurance_level": "medium",
	"project_name": "RunMaestro-Maestro",
	"tracked_extensions": [
		".js",
		".jsx",
		".mjs",
		".cjs",
		".ts",
		".tsx",
		".mts",
		".cts",
		".vue",
		".svelte",
		".astro",
		".html",
		".css",
		".scss",
		".sass",
		".less",
		".rs",
		".go",
		".c",
		".cpp",
		".h",
		".hpp",
		".zig",
		".java",
		".kt",
		".scala",
		".cs",
		".swift",
		".dart",
		".m",
		".py",
		".rb",
		".php",
		".lua",
		".sh",
		".ex",
		".exs",
		".sql",
		".graphql",
		".proto",
		".tf",
		".yaml",
		".yml",
		".toml"
	],
	"exclude_patterns": [
		"**/node_modules/**",
		"**/vendor/**",
		"**/.venv/**",
		"**/dist/**",
		"**/target/**",
		"**/.git/**",
		"**/build/**",
		"**/out/**",
		"**/__pycache__/**",
		"**/.next/**",
		"**/.nuxt/**",
		"**/.svelte-kit/**",
		"**/coverage/**",
		"**/.dart_tool/**",
		"**/Pods/**",
		"**/.build/**",
		"**/obj/**",
		"**/.gradle/**",
		"**/.terraform/**",
		"**/deps/**",
		"**/_build/**",
		"**/zig-out/**",
		"**/zig-cache/**"
	],
	"compress_reasoning_threshold_bytes": 10240,
	"external_blob_threshold_bytes": 102400
}
